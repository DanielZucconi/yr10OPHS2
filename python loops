count = 0
loop = "y"

while loop == "y":
    invite = input("Who do you want to invite to the party?" )
    print(invite + " has been invited")
    count += 1
    loop = input("Do you want to invite anyone else? Y or N")

print(str(count) + " people have been invited")





compNum = 50
num = int(input("Enter a number"))
count = 1

while num != 50:
    if num < 50:
        print("Low")
    if num > 50:
        print("High")
    num = int(input("Have another guess"))
    count += 1

print(f"Well done you took {count} attempts")






num = int(input("Enter a number between 10 and 20"))

while not 10 <= num <= 20:
    if num > 20:
        print("Too high")
    if num < 10:
        print("Too low")

    num = int(input("Try again "))

